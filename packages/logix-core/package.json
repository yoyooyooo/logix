      "./TraitLifecycle": {
        "types": "./dist/TraitLifecycle.d.ts",
        "import": "./dist/TraitLifecycle.js",
        "require": "./dist/TraitLifecycle.cjs"
      },
      "./internal/*": null
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit",
    "test": "vitest run",
    "test:changed": "vitest run --changed",
    "test:cache": "vitest run --cache",
    "test:watch": "vitest"
  },
  "dependencies": {
    "effect": "^3.19.8",
    "mutative": "^1.0.4"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
