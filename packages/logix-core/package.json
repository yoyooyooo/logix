{
  "name": "@logix/core",
  "version": "0.0.1",
  "description": "The pure logic engine for Logix",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./Actions": "./src/Actions.ts",
    "./Bound": "./src/Bound.ts",
    "./Debug": "./src/Debug.ts",
