{
  "name": "@logix/devtools-react",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "types": "src/index.tsx",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.tsx",
    "./*": "./src/*.tsx"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --noEmit",
    "dev": "tsc -p tsconfig.json --noEmit --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit"
  },
  "dependencies": {
    "@logix/core": "workspace:*",
    "effect": "^3.19.8",
    "react": "^19.2.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@logix/react": "workspace:*",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.3",
    "jsdom": "24.0.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.4.5",
    "vitest": "^4.0.15"
  }
}
