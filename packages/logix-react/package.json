{
  "name": "@logixjs/react",
  "version": "0.1.0",
  "files": [
    "dist/**"
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./internal/*": null
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js",
        "require": "./dist/*.cjs"
      },
      "./internal/*": null
    },
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "vitest run",
    "test:changed": "vitest run --changed",
    "test:cache": "vitest run --cache",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.test.json --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit"
  },
  "dependencies": {
    "@logixjs/core": "workspace:*",
    "effect": "^3.19.8",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@logixjs/test": "workspace:*",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.2.2",
    "@types/use-sync-external-store": "^1.5.0",
    "jsdom": "24.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  }
}
